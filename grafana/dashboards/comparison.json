{
  "id": null,
  "uid": "nd-compare",
  "title": "Networking Dashboard",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": { "from": "now-15m", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests Total (rate)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "Prometheus" }, "expr": "sum by(code)(rate(nd_requests_total[1m]))" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Temperature",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "Prometheus" }, "expr": "nd_temperature_celsius" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        { "refId": "A", "datasource": { "type": "prometheus", "uid": "Prometheus" }, "expr": "histogram_quantile(0.9, rate(nd_request_latency_seconds_bucket[5m]))" }
      ]
    }
  ],
  "templating": { "list": [] }
}